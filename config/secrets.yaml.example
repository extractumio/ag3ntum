# Ag3ntum Secrets Configuration
# Copy this file to secrets.yaml and add your actual API key:
#   cp secrets.yaml.example secrets.yaml
#
# The secrets.yaml file is gitignored and will not be committed.

# Anthropic API Key (required)
# Get yours at: https://console.anthropic.com/settings/keys
# Format: sk-ant-api03-...
anthropic_api_key: sk-ant-REPLACE_WITH_YOUR_API_KEY

# Fernet encryption key (auto-generated on first run)
# Used to encrypt user API keys in the Token table.
# Will be automatically generated if not present.
# fernet_key: <auto-generated>

# Note: Per-user JWT secrets are now stored in the database.
# The global jwt_secret has been removed for enhanced security.

